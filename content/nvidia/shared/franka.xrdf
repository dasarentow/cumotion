format: xrdf
format_version: 2.0

default_joint_positions:
  panda_joint1: 0.0
  panda_joint2: -1.3
  panda_joint3: 0.0
  panda_joint4: -2.87
  panda_joint5: 0.0
  panda_joint6: 2.0
  panda_joint7: 0.75
  panda_finger_joint1: 0.025
  panda_finger_joint2: 0.025

cspace:
  joint_names:
    - "panda_joint1"
    - "panda_joint2"
    - "panda_joint3"
    - "panda_joint4"
    - "panda_joint5"
    - "panda_joint6"
    - "panda_joint7"
  acceleration_limits: [15.0, 7.5, 10.0, 12.5, 15.0, 20.0, 20.0]
  jerk_limits: [7500.0, 3750.0, 5000.0, 6250.0, 7500.0, 10000.0, 10000.0]

tool_frames: ["panda_leftfingertip"]

world_collision:
  geometry: "franka_collision_spheres"

self_collision:
  geometry: "franka_self_collision_spheres"
  ignore:
    panda_link0: [ "panda_link1", "panda_link2" ]
    panda_link1: [ "panda_link2", "panda_link3", "panda_link4" ]
    panda_link2: [ "panda_link3", "panda_link4" ]
    panda_link3: [ "panda_link4", "panda_link6" ]
    panda_link4: [ "panda_link5", "panda_link6", "panda_link7" ]
    panda_link5: [ "panda_link6", "panda_link7", "panda_hand", "panda_leftfingertip", "panda_rightfingertip" ]
    panda_link6: [ "panda_link7", "panda_hand", "panda_leftfingertip", "panda_rightfingertip" ]
    panda_link7: [ "panda_hand", "panda_leftfingertip", "panda_rightfingertip" ]
    panda_hand: [ "panda_leftfingertip", "panda_rightfingertip" ]
    panda_leftfingertip: [ "panda_rightfingertip" ]

