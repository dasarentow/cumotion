format: xrdf
format_version: 2.0

cspace:
  joint_names:
    - "joint0"
    - "joint1"
  acceleration_limits: [10.0, 10.0]
  jerk_limits: [100.0, 100.0]

world_collision:
  geometry: "my_spheres"
  buffer_distance:
    frame0: 0.01
    frame1: 0.01
    frame1: 0.07

self_collision:
  geometry: "my_spheres"
  buffer_distance:
    frame0: 0.02
    frame1: 0.02

geometry:
  my_spheres:
    spheres:
      frame0:
        - center: [0, 0, 0]
          radius: 0.5
      frame1:
        - center: [0, 0, 0]
          radius: 0.6
