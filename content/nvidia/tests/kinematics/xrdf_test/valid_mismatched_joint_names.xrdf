format: xrdf
format_version: 2.0

default_joint_positions:
  joint0: 0.0
  joint_different: 0.0  # This doesn't match any joint in cspace - but is still valid

cspace:
  joint_names:
    - "joint0"
    - "joint1"
  acceleration_limits: [10.0, 20.0]
  jerk_limits: [100.0, 200.0]
