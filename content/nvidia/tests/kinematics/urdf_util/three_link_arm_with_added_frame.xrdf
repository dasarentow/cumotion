format: xrdf
format_version: 2.0

modifiers:
  - add_frame:
      frame_name: "my_new_end_effector"
      parent_frame_name: "link2"
      joint_name: "my_new_joint"
      joint_type: FIXED
      fixed_transform:
        position: [1.0, 0.0, 0.0]
        orientation: {w: 1.0, xyz: [0.0, 0.0, 0.0]}

default_joint_positions:
  joint0: 0.0
  joint1: 0.0
  joint2: 0.0

cspace:
  joint_names:
    - "joint0"
    - "joint1"
    - "joint2"
  acceleration_limits: [100.0, 100.0, 100.0]
  jerk_limits: [1000.0, 1000.0, 1000.0]

tool_frames: ["my_new_end_effector"]
